{
  "name": "node-typescript-project-template",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "clean": "rimraf ./build",
    "build": "npm-run-all test clean && tsc",
    "start": "node ./build/src",
    "local": "ts-node src",
    "local:watch": "nodemon src -e ts,json --exec 'npm run local'",
    "test": "jest",
    "test-verbose": "jest --verbose=true"
  },
  "keywords": [
    "typescript",
    "node",
    "jest",
    "supertest"
  ],
  "author": "Caglar Durmaz @ EgeCaglarDurmaz",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^29.4.0",
    "@types/node": "^18.14.0",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.52.0",
    "@typescript-eslint/parser": "^5.52.0",
    "jest": "^29.4.3",
    "nodemon": "^2.0.20",
    "npm-run-all": "^4.1.5",
    "rimraf": "^4.1.2",
    "supertest": "^6.3.3",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  },
  "dependencies": {}
}
